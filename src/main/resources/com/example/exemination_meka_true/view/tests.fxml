<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Label?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.exemination_meka_true.controller.TestController"
            style="-fx-background-color: #d9efd8;">
    <Label text="AVAILABLE TESTS" layoutX="150" layoutY="20"
           style="-fx-font-size: 18px; -fx-font-family: 'Arial'; -fx-text-fill: #006400;" />
    <ListView fx:id="testsList" layoutX="50" layoutY="60" prefWidth="400" prefHeight="300" />
    <Button text="Start Test" layoutX="180" layoutY="380" prefWidth="150"
            style="-fx-background-color: #006400; -fx-text-fill: white;" onAction="#selectTest" />
</AnchorPane>
